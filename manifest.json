{
	"manifest_version": 2,
	"author": "Kazina",
	"homepage_url": "https://github.com/KazinaG/Smart-Bookmark-Sorter",
	"name": "Smart bookmark sorter",
	"short_name": "SBS",
	"description": "Frees you from organizing your bookmarks.",
	"version": "2.3.1",
	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png",
		"48": "icon_48.png",
		"64": "icon_64.png",
		"128": "icon_128.png"
	},
	"browser_action": {
		"default_icon": "icon_128.png",
		"default_title": "Option",
		"default_popup": "popup/option.html"
	},
	"background": {
		"scripts": [
			"js/jQuery/jQuery-3.4.1.js",
			"js/constant.js",
			"js/option/receiver.js",
			"js/observer.js",
			"js/processor/setVisitPoint.js",
			"js/processor/bookmark-sorter.js",
			"js/processor/aggregate.js",
			"js/trigger.js"
		],
		"persistent": false
	},
	"permissions": [
		"bookmarks",
		"history",
		"storage"
	]
}